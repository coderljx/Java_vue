<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Dao.Memo.Memo">
    <select id="selectAll" resultType="JavaBean.memorandum">
        SELECT * FROM memorandum
    </select>

    <select id="selectByid" resultType="JavaBean.memorandum">
        SELECT * FROM memorandum where  id = #{id}
    </select>
</mapper>